= simple_form_for @ticket_codes_form, url: admin_ticket_codes_path, html: { class: 'form-horizontal' } do |f|
  = f.input :category, collection: @categories, value_method: :first, include_blank: false
  = f.input :propagator_id, collection: @propagators, include_blank: false
  = f.input :count

  .form-actions
    = f.button :submit, class: 'btn btn-primary'
    = link_to t('.cancel'), admin_ticket_codes_path, class: 'btn'
