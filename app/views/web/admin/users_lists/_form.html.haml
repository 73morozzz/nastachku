= simple_form_for @users_list, url: admin_users_lists_path, html: { class: 'form-horizontal' } do |f|
  - f.with_options input_html: { class: :span6 } do |f|
    = f.file_field :file
    %br
    %br
    %br
    = f.input :description, as: :string
  .form-actions
    = f.button :submit, class: 'btn-primary'
